{
  "name": "jellyplan",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/datastore": "3.12.9",
    "@aws-amplify/ui-react": "^3.5.0",
    "aws-amplify": "4.3.34",
    "framer-motion": "7.2.1",
    "luxon": "3.0.3",
    "react": "18.2.0",
    "react-calendar": "3.7.0",
    "react-dom": "18.2.0",
    "react-text-transition": "3.0.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "prepare": "npx husky install",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "amplify-modelgen": "node amplify\\scripts\\amplify-modelgen.js",
    "amplify-push": "node amplify\\scripts\\amplify-push.js"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.6",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "8.23.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-node": "4.1.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.5",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "vite": "^3.0.9"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "release": {
    "branches": [
      {
        "name": "main"
      },
      {
        "name": "dev",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "build/**",
            "dist/**"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
